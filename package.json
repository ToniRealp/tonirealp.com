{
    "name": "tonirealp.com",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "format": "prettier . --check --ignore-path .gitignore",
        "format:fix": "prettier . --write --ignore-path .gitignore",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "prepare": "husky install",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "*.(ts|tsx|js)": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.(md|json|mdx|yml)": "prettier --write"
    },
    "dependencies": {
        "graphql-request": "^3.7.0",
        "next": "12.0.7",
        "react": "17.0.2",
        "react-dom": "17.0.2"
    },
    "devDependencies": {
        "@formspree/react": "^2.2.4",
        "@headlessui/react": "^1.4.2",
        "@heroicons/react": "^1.0.5",
        "@tailwindcss/forms": "^0.4.0",
        "@types/node": "16.11.12",
        "@types/react": "17.0.37",
        "@typescript-eslint/eslint-plugin": "^5.6.0",
        "autoprefixer": "^10.4.0",
        "eslint": "8.4.1",
        "eslint-config-next": "12.0.7",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-unicorn": "^39.0.0",
        "eslint-plugin-unused-imports": "^2.0.0",
        "husky": "^7.0.4",
        "lint-staged": "^12.1.2",
        "postcss": "^8.4.5",
        "prettier": "2.5.1",
        "tailwindcss": "^3.0.7",
        "typescript": "4.5.3"
    }
}
